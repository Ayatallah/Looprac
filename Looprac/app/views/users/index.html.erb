<% unless current_user.blank? %>	
	<div class="container log-in">

		<div class="panel panel-default">
			<div class="panel-heading">
	    		<h3 class="main-color center">All Users</h3>
	    	</div>
	    	<div class="panel-body center">
	    		<% @users.each do |user| %>
				<%= link_to user.username, user %>
				<br>
				<% end %>
	    	</div>
		</div>
	</div>
<% end %>